{
  "name": "congresso-srmg",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "dev": "npm run clean && dotenv -e .env -- next dev",
    "prod": "npm run clean && dotenv -e .env.production -- next dev",
    "start": "dotenv -e .env.production -- next start",
    "build": "prisma generate && dotenv -e .env.production -- next build",
    "lint": "next lint",
    "seed": "node prisma/seed.js",
    "seed:test": "DATABASE_URL=\"$POSTGRES_PRISMA_URL?branch=test\" node prisma/seed.js",
    "seed:dev": "DATABASE_URL=\"$POSTGRES_PRISMA_URL?branch=dev\" node prisma/seed.js",
    "seed:main": "DATABASE_URL=\"$POSTGRES_PRISMA_URL?branch=main\" node prisma/seed.js",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "migrate:test": "DATABASE_URL=\"$POSTGRES_PRISMA_URL?branch=test\" prisma migrate dev --name test",
    "migrate:dev": "DATABASE_URL=\"$POSTGRES_PRISMA_URL?branch=dev\" prisma migrate dev --name dev",
    "migrate:main": "DATABASE_URL=\"$POSTGRES_PRISMA_URL?branch=main\" prisma migrate deploy",
    "generate": "npx prisma generate",
    "mongo:setup": "dotenv -e .env -- node app/scripts/setupMongoDB.js",
    "mongo:test": "dotenv -e .env.test -- node app/scripts/runCouponTests.js",
    "mongo:test-unit": "dotenv -e .env.test -- node app/scripts/runCouponTests.js --unit-only",
    "mongo:test-perf": "dotenv -e .env.test -- node app/scripts/runCouponTests.js --perf-only",
    "mongo:migrate": "dotenv -e .env -- node app/scripts/runCouponMigration.js",
    "mongo:migrate-validate": "dotenv -e .env -- node app/scripts/runCouponMigration.js --validate-only",
    "mongo:inspect": "dotenv -e .env -- node app/scripts/inspectCoupons.js",
    "test": "jest --config=jest.simple.config.cjs --forceExit",
    "test:unit": "jest --config=jest.simple.config.cjs tests/unit/ --forceExit",
    "test:integration": "jest --config=jest.simple.config.cjs tests/integration/ --forceExit",
    "test:simple": "jest --config=jest.simple.config.cjs tests/simple-integration.test.js --forceExit",
    "test:watch": "jest --config=jest.simple.config.cjs --watch --forceExit",
    "test:verbose": "jest --config=jest.simple.config.cjs --verbose --forceExit",
    "test:debug": "jest --config=jest.simple.config.cjs --detectOpenHandles --verbose",
    "test:legacy": "node --experimental-vm-modules docs/scripts/run-tests.cjs all",
    "test:unit-legacy": "node --experimental-vm-modules docs/scripts/run-tests.cjs unit",
    "test:integration-legacy": "node --experimental-vm-modules docs/scripts/run-tests.cjs integration",
    "test:jest": "node --experimental-vm-modules --no-warnings $(npm bin)/jest --config=jest.config.cjs",
    "test:with-data": "node --experimental-vm-modules docs/scripts/run-tests.cjs all --insert-data",
    "test:auth": "jest --config=jest.simple.config.cjs --testPathPatterns=auth --verbose --forceExit",
    "test:auth:watch": "jest --config=jest.simple.config.cjs --testPathPatterns=auth --watch --verbose",
    "test:auth:coverage": "jest --config=jest.simple.config.cjs --testPathPatterns=auth --coverage --verbose --forceExit",
    "test:auth:unit": "jest --config=jest.simple.config.cjs tests/unit/components/AuthGuard.test.js --verbose --forceExit",
    "test:auth:integration": "jest --config=jest.simple.config.cjs tests/integration/auth-*.test.js --verbose --forceExit",
    "test:auth:e2e": "jest --config=jest.simple.config.cjs tests/integration/auth-e2e.test.js --verbose --forceExit",
    "test:insert-data": "node docs/scripts/insert-test-data.cjs",
    "test:verify-data": "node docs/scripts/verify-mongodb-data.cjs",
    "test:clean-data": "node docs/scripts/clean-test-data.cjs"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@bull-board/api": "^6.7.10",
    "@bull-board/express": "^6.7.10",
    "@preact/signals-react": "^3.0.1",
    "@prisma/client": "^6.10.1",
    "@vercel/postgres": "^0.10.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "classnames": "^2.5.1",
    "cloudinary": "^2.5.0",
    "clsx": "^2.1.1",
    "dotenv-cli": "^8.0.0",
    "firebase": "^11.5.0",
    "jspdf": "^3.0.1",
    "mongodb": "^6.17.0",
    "mongoose": "^8.15.1",
    "next": "^14.2.30",
    "next-auth": "^4.24.7",
    "next-cloudinary": "^6.13.0",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.6.5",
    "pagarme": "^4.35.2",
    "pg": "^8.13.3",
    "qrcode.react": "^4.2.0",
    "react": "^18",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18",
    "react-icons": "^5.5.0",
    "react-select": "^5.10.0",
    "react-spinners": "^0.14.1",
    "sharp": "^0.33.5",
    "ts-jest": "^29.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@faker-js/faker": "^9.5.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "22.7.4",
    "@types/react": "18.3.10",
    "babel-jest": "^30.0.0",
    "cypress": "^14.4.1",
    "eslint": "8.57.1",
    "eslint-config-next": "15.3.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "postcss": "^8",
    "prisma": "^6.10.1",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.6.2"
  }
}
